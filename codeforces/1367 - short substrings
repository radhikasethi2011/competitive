#include <iostream>
#include <string>
#include <vector>
#include <bits/stdc++.h>
using namespace std;
int main() {
	//ios_base::sync_with_stdio(false);
	cin.tie(nullptr);
	cout.tie(nullptr);
	int t;
	cin>>t;
	while(t--)
	{
		string b;
		cin>>b;
		string a;
		int j = 0;
		for(int i = 1 ; i < b.length() ; i = i+2)
			a.push_back(b[i]);
      
		cout<<b[0]<<a<<endl;
	}
	return 0;
}
